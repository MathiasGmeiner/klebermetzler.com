{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@blocks/*": ["src/blocks/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@templates/*": ["src/templates/*"],
      "@scripts/*": ["src/scripts/*"],
      "@/*": ["./src/*"]
    }
  }
}
